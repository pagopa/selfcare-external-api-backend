{
  "openapi": "3.0.3",
  "info": {
    "title": "Billing Portal API service",
    "description": "This service is the proxy for the billing portal",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://${host}/${basePath}"
    }
  ],
  "tags": [
    {
      "name": "Onboarding"
    },
    {
      "name": "billing-portal"
    }
  ],
  "paths": {
    "/onboarding/{onboardingId}/recipient-code": {
      "put": {
        "tags": [
          "billing-portal",
          "Onboarding"
        ],
        "summary": "Update recipient code",
        "description": "Update recipient code receiving onboarding id.",
        "operationId": "updateOnboardingRecipientIdUsingPUT",
        "parameters": [
          {
            "name": "onboardingId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "recipientCode",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {}
            }
          },
          "401": {
            "description": "Not Authorized"
          },
          "403": {
            "description": "Not Allowed"
          }
        },
        "security": [
          {
            "apiKeyHeader": [
              "global"
            ]
          }
        ]
      }
    }
  },
  "components": {
    "schemas": {},
    "securitySchemes": {
      "SecurityScheme": {
        "type": "http",
        "description": "Authentication",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    }
  }
}